name: demo

on: 
  push:
    branches:
      - "demo"

jobs:
  recall-cache:
    runs-on: ubuntu-latest
    steps:
    - uses: actions/checkout@v3

    - uses: actions/setup-python@v3
      with:
        python-version: 3.9.12

    - uses: actions/cache@v3
      with:
        path: src 
        key: "${{ runner.os }}-build-${{ env.cache-name }}-${{ hashFiles('src/version_last.txt') }}"
        restore-keys: |
          ${{ runner.os }}-build-${{ env.cache-name }}
          ${{ runner.os }}-build
          ${{ runner.os }}
    - name: demo 
      run: python src/cache_read.py


  write-cache:
    runs-on: ubuntu-latest
    steps:
    - uses: actions/checkout@v3

    - uses: actions/setup-python@v3
      with:
        python-version: 3.9.12
    - name: demo 
      run: python src/cache_write.py
    - uses: actions/cache@v3
      with:
        path: src 
        key: "${{ runner.os }}-build-${{ env.cache-name }}-${{ hashFiles('src/version_last.txt') }}"
        restore-keys: |
          ${{ runner.os }}-build-${{ env.cache-name }}
          ${{ runner.os }}-build
          ${{ runner.os }}
